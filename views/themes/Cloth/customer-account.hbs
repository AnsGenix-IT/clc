    <div id="orderForm" class="accdata">
        <div class="col-12 bottom-pad-15 no-pad-right">
            <div class="card top-marg-15">
                <div class="card-body">
                    <h3 class="card-heading">{{ @root.__ "Orders" }}</h3>
                    <div id="accordion">
                        {{#unless orders}}
                        <div class="card mb-3">
                            <div class="card-body">
                                There are no orders for this account. <a class="text-success" href="/">Order
                                    here</a>
                            </div>
                        </div>
                        {{/unless}}
                        {{#each orders}}
                        <div class="card mb-3">
                            <div class="card-header" id="headingOne">
                                <p class="mb-0">
                                    OrderId: {{this._id}} -
                                    Date: {{formatDate this.orderDate "DD/MM/YYYY hh:mmA"}}
                                    <button class="btn btn-sm btn-outline-success float-right" data-toggle="collapse"
                                        data-target="#collapse{{this._id}}" aria-expanded="true"
                                        aria-controls="collapse{{this._id}}">
                                        View
                                    </button>
                                </p>
                            </div>
                            <div id="collapse{{this._id}}" class="collapse orderScroll" aria-labelledby="headingOne"
                                data-parent="#accordion">
                                <div class="card-body">
                                    <div class="formlevel">
                                        {{#ifCond this.orderStatus "==" "Completed"}}
                                        <div class="stages">                 
                                                <label for="one" class="check">&check; <span class="titlelevel">Booked</span></label>
                                                <label for="two" class="check">&check; <span class="titlelevel">PickUp</span></label>
                                                <label for="three" class="check">&check; <span class="titlelevel">Serviced</span></label>
                                                <label for="four" class="check active">&check; <span class="titlelevel">Completed</span></label>
                                            </div>
                                                <span class="progress1 width4"><span></span></span>
                                        {{/ifCond}}
                                        {{#ifCond this.orderStatus "==" "Service Completed"}}
                                        <div class="stages">                 
                                                <label for="one" class="check">&check; <span class="titlelevel">Booked</span></label>
                                                <label for="two" class="active">&check; <span class="titlelevel">PickUp</span></label>
                                                <label for="three" class="active check">&check; <span class="titlelevel">Serviced</span></label>
                                                <label for="four">4 <span class="titlelevel">Completed</span></label>
                                            </div>
                                                <span class="progress1 width3"><span></span></span>
                                        {{/ifCond}}
                                        {{#ifCond this.orderStatus "==" "Vehicle PickUp"}}
                                        <div class="stages">                 
                                                <label for="one" class="check">&check; <span class="titlelevel">Booked</span></label>
                                                <label for="two" class="active check">&check; <span class="titlelevel">PickUp</span></label>
                                                <label for="three">2 <span class="titlelevel">Serviced</span></label>
                                                <label for="four">4 <span class="titlelevel">Completed</span></label>
                                            </div>
                                                <span class="progress1 width2"><span></span></span>
                                        {{/ifCond}}
                                        {{#ifCond this.orderStatus "==" "Paid"}}
                                        <div class="stages">                 
                                                <label for="one" class="active check">&check; <span class="titlelevel">Booked</span></label>
                                                <label for="two">2 <span class="titlelevel">PickUp</span></label>
                                                <label for="three">2 <span class="titlelevel">Serviced</span></label>
                                                <label for="four">4 <span class="titlelevel">Completed</span></label>
                                            </div>
                                                <span class="progress1 width1"><span></span></span>
                                        {{/ifCond}}
                                        {{#ifCond this.orderStatus "==" "Booked"}}
                                        <div class="stages">                 
                                                <label for="one" class="active check">&check; <span class="titlelevel">Booked</span></label>
                                                <label for="two">2 <span class="titlelevel">PickUp</span></label>
                                                <label for="three">2 <span class="titlelevel">Serviced</span></label>
                                                <label for="four">4 <span class="titlelevel">Completed</span></label>
                                            </div>
                                                <span class="progress1 width1"><span></span></span>
                                        {{/ifCond}}
                                        {{#ifCond this.orderStatus "==" "Accepted"}}
                                        <div class="stages">                 
                                                <label for="one" class="active check">&check; <span class="titlelevel">Booked</span></label>
                                                <label for="two">2 <span class="titlelevel">PickUp</span></label>
                                                <label for="three">2 <span class="titlelevel">Serviced</span></label>
                                                <label for="four">4 <span class="titlelevel">Completed</span></label>
                                            </div>
                                                <span class="progress1 width1"><span></span></span>
                                        {{/ifCond}}
                                    </div>
                                    {{#ifCond this.orderStatus "==" "Service Completed"}}
                                    <div class="paynow">
                                        <button class="btn btn-primary" id="razorpay" data-id="{{../this._id}}">Pay Now</button>
                                    </div>
                                    {{/ifCond}}
                                    <ul class="list-group">
                                        <li class="list-group-item list-group-input-pad">
                                            <strong class="float-left"> Order status: </strong><span
                                                class="text-{{getStatusColor this.orderStatus}} float-right">{{this.orderStatus}}</span>
                                        </li>
                                        <li class="list-group-item"><strong class="float-left"> {{ @root.__ "Order date" }}:
                                            </strong><span
                                                class="float-right">{{formatDate result.orderDate "DD/MM/YYYY hh:mmA"}}</span>
                                        </li>
                                        <li class="list-group-item"><strong class="float-left"> {{ @root.__ "Order ID" }}:
                                            </strong><span class="float-right">{{this._id}}</span></li>
                                        {{#if result.orderExpectedBtc }}
                                        <li class="list-group-item"><strong class="float-left"> {{ @root.__ "Order Expected BTC" }}:
                                            </strong><span class="float-right">{{this.orderExpectedBtc}}</span></li>
                                        {{/if}}
                                        {{#if this.orderReceivedBtc }}
                                        <li class="list-group-item"><strong class="float-left"> {{ @root.__ "Order Received BTC" }}:
                                            </strong><span class="float-right">{{this.orderReceivedBtc}}</span></li>
                                        {{/if}}
                                        {{#if this.orderBlockonomicsTxid }}
                                        <li class="list-group-item"><strong class="float-left">
                                                {{ @root.__ "Order Blockonomics Txid" }}: </strong><span
                                                class="float-right">{{this.orderBlockonomicsTxid}}</span></li>
                                        {{/if}}
                           <li class="list-group-item"><strong class="float-left"> Supervisor Name:
                                            </strong><span
                                                class="float-right">{{this.supervisor.[0].Name}}</span>
                                        </li>
                                        <li class="list-group-item"><strong class="float-left"> {{ @root.__ "Supervisor License Number" }}:
                                            </strong><span
                                                class="float-right">{{this.supervisor.[0].AddharNumber}}</span>
                                        </li>
                                        <li class="list-group-item"><strong class="float-left"> {{ @root.__ "Supervisor Phone Number" }}:
                                            </strong><span
                                                class="float-right">{{this.supervisor.[0].PhoneNumber}}</span>
                                        </li>
                                        <li class="list-group-item"><strong class="float-left"> Vehicle Number:
                                            </strong><span
                                                class="float-right">{{this.orderVehicleNumber}}</span>
                                        </li>
                                        <li class="list-group-item"><strong class="float-left"> Vehicle Type:
                                            </strong><span
                                                class="float-right">{{this.orderType}}</span>
                                        </li>
                                        <li class="list-group-item"><strong class="float-left"> Order Slot Date:
                                            </strong><span
                                                class="float-right">{{this.orderSlotdate}}</span>
                                        </li>
                                        <li class="list-group-item"><strong class="float-left"> Order Slot Time:
                                            </strong><span
                                                class="float-right">{{this.orderSlottime}}</span>
                                        </li>
                                        <li class="list-group-item"><strong class="float-left"> Order Vehicle:
                                            </strong><span
                                                class="float-right">{{this.orderCarTitle}} {{this.orderModelName}}</span>
                                        </li>
                                         <li class="list-group-item"><strong class="float-left"> Order Service Station:
                                            </strong><span
                                                class="float-right">{{this.orderServiceCenter}}</span>
                                        </li>
                                        <li class="list-group-item"><strong class="float-left"> {{ @root.__ "Email address" }}:
                                            </strong><span class="float-right">{{this.orderEmail}}</span></li>

                                        <li class="list-group-item"><strong class="float-left"> {{ @root.__ "Name" }}:
                                            </strong><span class="float-right">{{this.orderFirstname}} {{this.orderLastname}}</span></li>
                                        <li class="list-group-item"><strong> {{ @root.__ "Pick Up Info" }}: </strong><span class="float-right">Name: {{this.orderPickup.name}}, Phone {{this.orderPickup.phone}},Address {{this.orderPickup.address}},City {{this.orderPickup.city}}, {{this.orderPickup.postcode}}</span></li>
            <li class="list-group-item"><strong> {{ @root.__ "Drop Info" }}: </strong><span class="float-right">Name: {{this.orderDrop.name}}, Phone {{this.orderDrop.phone}},Address {{this.orderDrop.address}},City {{this.orderDrop.city}}, {{this.orderDrop.postcode}}</span></li>
                                        <li class="list-group-item"><strong class="float-left"> {{ @root.__ "Phone number" }}:
                                            </strong><span class="float-right">{{this.orderPhoneNumber}}</span></li>
                                        <li class="list-group-item">&nbsp;</li>
                                        <li class="list-group-item"><strong
                                                class="text-info">{{ @root.__ "Products ordered" }}</strong></li>
                                        {{#each this.orderProducts}}
                                        <li class="list-group-item">
                                            {{this.quantity}} x {{this.title}}
                                            {{#if this.variantId}}
                                            &nbsp; > &nbsp;
                                            <span class="text-warning"> {{ @root.__ "Options" }}: </span>
                                            {{this.variantTitle}}
                                            {{/if}}
                                            <div class="float-right">
                                                {{currencySymbol @root.config.currencySymbol}}{{formatAmount this.totalItemPrice}}
                                            </div>
                                            {{#if productComment}}
                                            <h4><span class="text-danger">Comment:</span> {{this.productComment}}
                                            </h4>
                                            {{/if}}
                                        </li>
                                        {{/each}}Paid
                                    </ul>
                                </div>
                            </div>
                        </div>
                        {{/each}}
                    </div>
                </div>
            </div>
        </div>
    </div>
  </div>
    </div>
</div>
