    <div id="orderForm1" class="accdata">
        <div class="col-12 bottom-pad-15 no-pad-right">
            <div class="card top-marg-15">
                <div class="card-body">
                    <h3 class="card-heading">{{ @root.__ "Pickup Address" }}</h3>
                    <div id="accordion" class="customeraddress">
                        {{#unless customerArray.pickupaddress}}
                        <div class="card mb-3">
                            <div class="card-body">
                                There are no Address for this account. <a class="text-success" href="/">Add Address
                                    here</a>
                            </div>
                        </div>
                        {{/unless}}
                        {{#each customerArray.pickupaddress}}
                        
                        <div class="address">
                  <div class="number">
                    {{#ifCond @root.session.customerpickaddress "==" this.addressline}}
                    <div>
                      <img src="/images/tick1.png" alt="Choose Address">
                    </div>
                    {{/ifCond}}
                    <div>
                      <h4>{{this.name}} {{this.phone}}</h4>
                    </div>
                  </div>
                  <div class="add">
                 {{this.addressline}} {{this.city}}, {{this.pincode}}
                  </div>
             
                  <div class="button">
                      <a href="/customer/userEditPick/{{@key}}" data-id="{{@key}}">Edit</a>
                    <button id="pickDelete" data-id="{{@key}}" class="pickDelete">Delete</button>
                  </div>
                </div>
                <hr>
                        {{/each}}
                    </div>
                </div>
            </div>
        </div>
    </div>


